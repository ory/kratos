// tslint:disable
declare namespace {{ namespace }} {
  {% for type in list -%}
{%- if type.props.length %}
  export interface {{ type.typeName | safe }} {
    {%- for prop in type.props %}
    {%- if prop.desc %}
    /** {{ prop.desc }} */
    {%- endif %}
    {{ prop.name }}{{ '' if prop.required else '?' }}: {{ prop.type | safe }};
    {%- endfor %}
  }
{%- else %}
  export type {{ type.typeName | safe }} = {{ type.type }};
{%- endif %}
{% endfor %}
}
