{
  "$id": "https://schemas.ory.sh/kratos/selfservice/strategy/webauthn/login.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "csrf_token": {
      "type": "string"
    },
    "webauthn_login": {
      "type": "string"
    },
    "method": {
      "type": "string"
    },
    "identifier": {
      "type": "string",
      "minLength": 1
    }
  },
  "oneOf": [
    {
      "required": [
        "webauthn_login"
      ]
    },
    {
      "required": [
        "identifier",
        "method"
      ]
    }
  ]
}
