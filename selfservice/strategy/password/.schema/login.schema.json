{
  "$id": "https://schemas.ory.sh/kratos/selfservice/strategy/password/login.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "password",
    "method"
  ],
  "properties": {
    "csrf_token": {
      "type": "string"
    },
    "password": {
      "type": "object",
      "required": [
        "password",
        "identifier"
      ],
      "properties": {
        "password": {
          "type": "string",
          "minLength": 1
        },
        "identifier": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "method": {
      "type": "string"
    }
  }
}
