{
  "$id": "https://schemas.ory.sh/kratos/selfservice/strategy/password/settings.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "password"
  ],
  "properties": {
    "csrf_token": {
      "type": "string"
    },
    "method": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "minLength": 1
    },
    "transient_payload": {
      "type": "object",
      "additionalProperties": true
    }
  }
}
